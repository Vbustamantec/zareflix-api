{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"rootDir": "./src",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"typeRoots": [
			"./node_modules/@types",
			"src/**/*.ts",
			"src/types/**/*.d.ts"
		],
		"types": ["express", "node"]
	},
	"include": ["src/**/*"]
}
